align-items: center;
margin-top: 20px;
}

.pagination button {
background-color: #f5f5f5;
border: 1px solid #ccc;
padding: 6px 12px;
margin: 0 3px;
cursor: pointer;

transition: background-color 0.3s, color 0.3s;
}

.pagination button.active {
background-color: #007bff;
color: #fff;
border-color: #007bff;
}

.pagination button:hover {
background-color: #ddd;
}

 /* CSS for Circular Zoom Effect */
.zoom-container {
    position: relative;
    overflow: hidden;
}

.zoom-image {
    transition: transform 0.3s ease-in-out;
}

.thumbnail-image {
    height: 150px;
    width: 100px;
}

.zoom-container::before {
    content: "";
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 0;
    height: 0;
    background: rgba(255, 255, 255, 0.7);
    border-radius: 50%;
    opacity: 0;
    transition: all 0.3s ease-in-out;
}
/* Custom button styles */
.custom-btn {
  background-color: transparent;
  border: 1px;
  padding: 0;
  cursor: pointer; /* Add pointer cursor on hover for better user feedback */
  outline: none; /* Remove the default button outline on focus */
}

/* Style the icons */
.custom-btn i {
  font-size: 16px; /* Adjust the font size as needed */
  vertical-align: middle; /* Center the icon vertically */
}

/* Style the number and <i> tag as black */
.custom-btn i,
.custom-btn .qty-val {
  color: black;
}



.zoom-container:hover .zoom-image {
    transform: scale(1.1); /* Adjust the scale factor as needed for your desired zoom level */
}

.zoom-container:hover::before {
    width: 100%;
    height: 100%;
    opacity: 1;
}
.product-price {
    display: flex;
    justify-content: space-between;
}

.price-container {
    flex-grow: 1; /* Allow the container to grow and push the <a> tag to the right */
}
